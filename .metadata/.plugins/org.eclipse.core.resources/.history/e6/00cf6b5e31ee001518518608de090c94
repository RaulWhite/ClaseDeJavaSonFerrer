 * @author RaÃºl Blanco Rama
 */
public class Ejercicio1 {

	public static void main(String[] args) {
		
		System.out.println(transforma(95));
		
	}
	
	public static String transforma(int dato){
		String binario = "";
		if (dato > 1){
			binario = binario + "" + (dato%2);
			return binario + "" + transforma(dato/2);
		} else {
			return binario + "" + dato;
		}
	}
	
}
