 */
public class Ejercicio1 {

	public static void main(String[] args) {
		
		System.out.println(transforma(95));
		
	}
	
	public static int transforma(int dato){
		String binario = "";
		if (dato > 1){
			binario = binario + "" + (dato%2);
			return Integer.parseInt(transforma(dato/2) + binario);
		} else {
			return Integer.parseInt(dato + "" + binario);
		}
	}
	
}
