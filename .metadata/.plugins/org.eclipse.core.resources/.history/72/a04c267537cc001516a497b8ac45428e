import java.util.Scanner;

public class EjemplosArrays {
	
	static String[] palabras = new String[10];

	public static void main(String[] args) {
		
		
		
	}
	
	static void relleno(){
		
		for (int i = 0; i < palabras.length; i++){
			System.out.println("Palabra " + (i+1));
		}
		
	}
	
}
