package excepciones;

/**
 * @author Raúl Blanco Rama
 */

public class PruebaExcepciones {

	public static void main(String[] args) {
		
		String entrada = "12";
				
		try {
			int numero = Integer.parseInt(entrada);
			System.out.println(numero);
		} catch (Exception e) {
			System.out.println("Eso no es un número");
		}
		
	}
	
}
