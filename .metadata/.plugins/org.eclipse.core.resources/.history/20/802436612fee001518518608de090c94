 */
public class Ejercicio1 {

	public static void main(String[] args) {
		
		System.out.println(transforma(100));
		
	}
	
	public static int transforma(int dato){
		int binario = 0;
		if (dato > 1){
			binario = (binario*10) + (dato%2);
			dato/2;
			return;
		} else {
			return;
		}
	}
	
}
