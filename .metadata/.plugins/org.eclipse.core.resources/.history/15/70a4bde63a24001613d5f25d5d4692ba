public class Copia {

	public static void main(String[] args) {
		
		try {
			FileInputStream entrada = new FileInputStream("logo.jpg");
			FileOutputStream copia = new FileOutputStream("logo2.jpg");
			
			int byteLeido = 0; 
			
			boolean eof = false;
			
			while (eof == false) {
				byteLeido = entrada.read();
				
				copia.write(byteLeido);
				
				if (entrada.read())
			}			
			
		} catch (IOException ioe) {
			ioe.printStackTrace();
		}
		
	}
	
}
