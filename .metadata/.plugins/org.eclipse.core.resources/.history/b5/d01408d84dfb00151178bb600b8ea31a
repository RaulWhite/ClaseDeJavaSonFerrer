 */
public class Ejercicio2 {

	public static void main(String[] args) {
		
		System.out.println(transforma(100));
		
	}
	
	public static long transforma(int dato){
		String binario = "";
		for (int i = dato; i > 0; i/=2){
			binario = (i%2) + binario;
		}
		return Integer.parseInt(binario);
	}
	
}
