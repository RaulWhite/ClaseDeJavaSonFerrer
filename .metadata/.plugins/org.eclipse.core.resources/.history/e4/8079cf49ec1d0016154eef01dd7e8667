public class Libros extends JFrame implements ItemListener {

	public static void main(String[] args) {
		
		new Libros();
		
	}
	
	public Libros() {
		
		super("Libro y autor");
		setSize(300, 80);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
