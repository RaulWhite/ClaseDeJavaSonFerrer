 */

package Poker;

public class Carta {
	
	// Atributos	
	private int valor;
	private int palo;

	//Constructores
	
	
	
	// Métodos

	public int getValor() {
		return valor;
	}
	
	public void setValor(int valor) {
		this.valor = valor;
	}

	public int getPalo() {
		return palo;
	}

	public void setPalo(int palo) {
		this.palo = palo;
	}
	
}
