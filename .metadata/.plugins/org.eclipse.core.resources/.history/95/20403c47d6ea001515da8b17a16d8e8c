/**
 * @author RaÃºl Blanco Rama
 */
public class Fibonacci {

	public static void main(String[] args) {
		
		System.out.println(Fibonacci.fibonacci(5));
		
	}
	
	int fibonacci(int n){
		if (n == 1){
			return 1;
		} else if (n == 2) {
			return 1;
		} else if (n > 2){
			return (fibonacci(n-1) + fibonacci(n-2));
		}
		return 0;
	}
	
}
