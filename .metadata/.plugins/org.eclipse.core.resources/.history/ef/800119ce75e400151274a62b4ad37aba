package algebra;
/**
 * @author Raúl Blanco Rama
 */

public class Polinomio {

	// Atributos
	private double coeficientes[];
	
	// Constructores
	Polinomio(){
		for (int i = 0; i <= 2; i++){
			coeficientes[i] = 0;
		}
	}
	
	Polinomio (int grado){
		for (int i = 0; i <= grado; i++){
			coeficientes[grado] = 0;
		}
	}
	
	Polinomio (double[] coefs){
		coefs = coeficientes;
	}
	
	// Métodos
	int getGrado(){
		return coeficientes.length - 1;
	}
	
}
