 * @author Ra√∫l Blanco Rama
 */
public class Ejercicio3 {

	public static void main(String[] args) {
		
		double cotizaciones[] = {47.5, 47.8, 48, 48.1, 47.7, 47.3, 46, 47.9, 48.05, 48.7};
		double aux;
		int posMin = 0;
		
		for (int i = 1; i < cotizaciones.length; i++){
			for (int j = i; j < cotizaciones.length; j++){
				if (cotizaciones[i] < cotizaciones[posMin]){
					posMin = j;
				}
			}
			aux = cotizaciones[posMin];
			cotizaciones[posMin] = cotizaciones[i];
			cotizaciones[i] = aux;
		}
		
		for (int k = 0; k < cotizaciones.length; k++){
			System.out.print(cotizaciones[k] + " ");
		}
		
	}
	
}
