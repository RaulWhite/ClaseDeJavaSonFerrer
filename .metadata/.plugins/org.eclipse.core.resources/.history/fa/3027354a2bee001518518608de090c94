public class Ejercicio1 {

	public static void main(String[] args) {
		
		transforma(100);
		
	}
	
	public static void transforma(int dato){
		if (dato > 1){
			dato/2;
			System.out.print(dato%2);
			return;
		} else {
			System.out.print(dato%2);
			return;
		}
	}
	
}
