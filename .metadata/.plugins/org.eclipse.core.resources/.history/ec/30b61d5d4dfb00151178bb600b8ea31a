public class Ejercicio1 {

	public static void main(String[] args) {
		
		System.out.println(transforma(100));
		
	}
	
	public static int transforma(int dato){
		String binario = "";
		if (dato > 1){
			binario = ;
			return Integer.parseInt("" + transforma(dato/2) + (dato%2));
		} else {
			return dato;
		}
	}
	
}
