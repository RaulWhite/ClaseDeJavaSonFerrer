public class Ejercicio1 {

	public static void main(String[] args) {
		
		System.out.println(transforma(100));
		
	}
	
	public static int transforma(int dato){
		if (dato > 1){
			transforma(dato/2);
			System.out.print(dato%2);
			return;
		} else {
			System.out.print(dato%2);
			return;
		}
	}
	
